class Txt2Png
# Font::BDF {{{
module Font
	class BDF
		Glyph = Struct.new(:bitmap, :move_x, :move_y)

		def initialize(font)
			@glyph = Array.new

			if font.is_a?(IO)
				parse(font)
			elsif font.is_a?(String) and /^STARTFONT / =~ font
				require 'stringio'
				parse StringIO.new(font)
			else
				File.open(font) do |f|
					parse(f)
				end
			end
		end

		attr_reader :x, :y

		def parse(f)
			char = 0

			while line = f.gets
				case line
					when /^FONTBOUNDINGBOX ([0-9]+) ([0-9]+)/
						@x = $1.to_i
						@y = $2.to_i

					when /^STARTCHAR (.*)/
						@x.nil? or @y.nil? and raise "Parse error: STARTCHAR before FONTBOUNDINGBOX"
						parse_char(f, $1)
				end
			end
		end

		def parse_char(f, name)
			encoding = name.to_i
			bitmap = []
			bbX = @x
			bbY = @y
			bbXoff = 0 # TODO add support for arbitary offset glyphs.
			bbYoff = 0 # TODO
			moveX  = @x
			moveY  = @y

			while line = f.gets
				case line
					when /^ENCODING ([0-9]+)/
						encoding = $1.to_i

					when /^DWIDTH ([-0-9]+) ([-0-9]+)/
						moveX = $1.to_i
						moveY = $2.to_i

					when /^BBX ([0-9]+) ([0-9]+) ([-0-9]+) ([-0-9]+)/
						bbX    = $1.to_i
						bbY    = $2.to_i
						bbXoff = $3.to_i
						bbYoff = $4.to_i

					when /^BITMAP/
						enc = []
						y.times do
							line = f.gets.chomp
							if /^[0-9A-Fa-f]+/ !~ line
								raise "Parse error in char: not hex data"
							end
							enc << line
						end
						line = f.gets
						if /^ENDCHAR/ !~ line
							raise "Parse error in char: ENDCHAR not where expected (#{line})"
						end

						enc.each do |line|
							bit_count = 0
							row = []
							line.each_byte do |b|
								int = Integer("0x#{b.chr}")
								$stderr.puts "%b" % int if bit_count > 4

								3.downto(0) do |i| # each bit
									break if (bit_count) > (@x)
									row << ((int & (1 << i)) != 0)
									bit_count += 1
								end
							end

							bitmap << row
						end
						break # end processing
				end
			end

			@glyph[encoding] = Glyph.new(bitmap, moveX, moveY)
		end

		def [](char)
			@glyph[char]
		end

		def to_gd
			require 'GD'
			x, y = 20 * (@x + 6), (256 / 20.0).ceil * (@y + 6)

			glyphs = GD::Image.new(x, y)
			chr_white = glyphs.colorResolve(255,255,255)
			chr_black = glyphs.colorResolve(0,0,0)

			32.upto(255) do |b|
				table_x,table_y = *get_asc_location(b)

				@y.times do |y|
					@x.times do |x|
						glyphs.setPixel(table_x + x, table_y + y,
						    (self[b].bitmap[y][x] ? chr_black : chr_white))
					end
				end
			end

			glyphs
		end

		def get_asc_location(ascii)
			ascii -= 32

			col = ascii % 20
			col = 20 if col == 0

			row = ((ascii - col) / 20.0).ceil

			x = ((col) * @x)
			y = ((row) * @y)

			[x, y]
		end
	end
end
# }}}

	def self.to_png(string) # {{{
		@font   ||= Font::BDF.new(DATA)
		@glyphs ||= @font.to_gd

		# Calculate width and height in characters
		x = 0
		y = 0
		string.each_line do |line|
			w = line.chomp.length
			x = w if w > x
			y += 1
		end

		# Calculate width and height in pixels
		x = (@font.x * x) + 6
		y = (@font.y * (string.count("\n") + 1)) + 6

		img = GD::Image.new(x,y)

		white = img.colorResolve(255,255,255)
		black = img.colorResolve(0,0,0)
		#img.transparent(white)
		img.fill(0,0, white)

		# For each pixel, find it's glyph and drop it on the image
		x,y = 3,3 # Starting point

		string.each_byte do |b|
			next if b == ?\r # skip \r, assuming there's an \n
			if b == ?\n
				x = 3 # reset to start of line
				y += @font.y # go down a line
				next
			end

			# Copy the glyph onto our image
			pos = @font.get_asc_location(b)
			@glyphs.copy(img, x, y, pos[0], pos[1], @font.x, @font.y)

			x += @font[b].move_x # Next character
			y += @font[b].move_y
		end

		png = img.pngStr
		img.destroy
		png
	end # }}}
end

class NfoController
	Query = 'SELECT n_body FROM NFOs WHERE n_fileid = ?'

	def initialize
		@db = DBI.connect('dbi:Mysql:newzDog:db-mgt.newzbin.com', 'newzDog', '...')
	end

	def handle_cgi(cgi)
		raise ArgumentError if cgi['id'].nil?

		nfo = @db.select_one(Query, cgi['id'].to_i)
		if nfo
			if cgi['fmt'] == 'png'
				png = Txt2Png.to_png(nfo['n_body'])
				cgi.out('image/png') { png }
			else
				cgi.out('text/plain') { nfo['n_body'] }
			end
		else
			cgi.out('status' => 'NOT_FOUND') { "Unable to locate an nfo with that ID" }
		end
	end
end

log = Syslog::open("nfo.fcgi", Syslog::LOG_PID|Syslog::LOG_CONS, Syslog::LOG_LOCAL4)

site = NfoController.new

errors = 0

FCGI.each_cgi do |cgi|
	begin
		site.handle_cgi(cgi)
		errors = 0
	rescue DBI::DatabaseError => e
		log.info("Database error: %s \"%s\"", e.inspect, e.message)
	rescue Exception => e
		errors += 1
		log.crit("Exception: %s: \"%s\", backtrace: %s",
		             e.inspect, e.message, e.backtrace.join(", "))
		begin
			cgi.out('status' => 'SERVER_ERROR') { "" }
		rescue => e # normally Errno::ECONNRESET
			log.info("Exception: %s on cgi.out in exception", e.inspect)
		end
		if errors > 5
			log.crit("Hit exception limit, dying :(")
			break
		end
	end
end



